# Sample TuskLang configuration for Java testing
app_name: "TuskLang Java Demo"
version: "1.0.0"
debug: true
port: 8080

# Database configuration
database:
  host: "localhost"
  port: 5432
  name: "tusklang_db"
  user: "admin"
  password: "secret123"

# Feature flags
features:
  - logging
  - metrics
  - hot_reload
  - caching

# API configuration
api:
  version: "v1"
  rate_limit: 1000
  timeout: 30

# Logging configuration
logging:
  level: "info"
  format: "json"
  output: "stdout"

# Cache settings
cache:
  type: "redis"
  host: "localhost"
  port: 6379
  ttl: 3600 